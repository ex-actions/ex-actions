{
  "name": "ex-actions",
  "version": "0.0.0",
  "description": "Github Actions for Elixir projects.",
  "main": "src/setup-mix/entry.ts",
  "scripts": {
    "build": "node ./esbuild.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src/**/*.ts",
    "tsc": "tsc --noEmit",
    "test": "jest",
    "watch": "npm run build -- --watch"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions"
  ],
  "author": "Benjamin Stepp",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^3.0.4",
    "@actions/core": "^1.10.0",
    "@actions/exec": "^1.1.1",
    "@actions/glob": "^0.3.0",
    "@actions/io": "^1.1.2"
  },
  "devDependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.10.5",
    "@typescript-eslint/parser": "^4.33.0",
    "esbuild": "^0.14.54",
    "eslint": "^7.32.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.3.2",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.4.4"
  }
}
